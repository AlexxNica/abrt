#: ../src/cli/cli.c:48
msgid ""
"& [-vsp] -L[PREFIX] [DUMP_DIR]\n"
"   or: & [-vsp] -e EVENT DUMP_DIR\n"
"   or: & [-vsp] -a[y] DUMP_DIR\n"
"   or: & [-vsp] -c[y] DUMP_DIR\n"
"   or: & [-vsp] -r[y|o|d] DUMP_DIR"
msgstr ""
"& [-vsp] -L[PRZEDROSTEK] [KATALOG_ZRZUTU]\n"
"   lub: & [-vsp] -e ZDARZENIE KATALOG_ZRZUTU\n"
"   lub: & [-vsp] -a[y] KATALOG_ZRZUTU\n"
"   lub: & [-vsp] -c[y] KATALOG_ZRZUTU\n"
"   lub: & [-vsp] -r[y|o|d] KATALOG_ZRZUTU"

