msgstr "Controlla la presenza dei file vimrc e gvimrc in /etc archiviandoli rispettivamente come system_vimrc e system_gvimrc."

#: ../src/plugins/collect_vimrc_user.xml.in.h:1
msgid "Collect yours vim configuration files"
msgstr "Acquisisce i tuoi file di configurazione vim"

#: ../src/plugins/collect_vimrc_user.xml.in.h:2
msgid "Save .vimrc and .gvimrc from your home directory"
msgstr "Salva .vimrc e .gvimrc dalla tua directory home"

