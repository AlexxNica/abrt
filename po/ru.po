#: ../src/plugins/collect_vimrc_system.xml.in.h:2
msgid "Save /etc/vimrc and /etc/gvimrc"
msgstr "Чтение /etc/vimrc и /etc/gvimrc"

#: ../src/plugins/collect_vimrc_system.xml.in.h:3
